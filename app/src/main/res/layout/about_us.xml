<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    xmlns:android="http://schemas.android.com/apk/res/android" >
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="64dp"
    android:background="@color/colorPrimary">
    <ImageView
        android:id="@+id/backArrow"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="24dp"
        android:elevation="2dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="?selectableItemBackground"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="60dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_arrow" />

    <ImageView
        android:layout_width="match_parent"
        android:id="@+id/image"
        android:layout_height="300dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/img2"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        app:layout_constraintTop_toBottomOf="@id/image"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/A"
        android:layout_height="wrap_content"
        android:textColor="#F0D453"
        android:textSize="42sp"
        android:textStyle="normal"
        android:layout_marginLeft="24dp"
        android:fontFamily="@font/montserratmedium"
        android:text="A"
        />
    <TextView
        app:layout_constraintStart_toEndOf="@id/A"
        app:layout_constraintTop_toBottomOf="@id/image"
        android:layout_width="wrap_content"
        android:id="@+id/other"
        android:textStyle="normal"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:textSize="42sp"
        android:fontFamily="@font/montserratmedium"
        android:text="bout Us"
        />
    <TextView
        android:layout_width="match_parent"
        android:id="@id/info"
        app:layout_constraintTop_toBottomOf="@id/other"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserratmedium"
        android:textSize="12sp"
        android:textStyle="normal"
        android:textColor="#FFFFFF"
        android:layout_marginHorizontal="24dp"
        android:layout_marginTop="16dp"
        android:text="Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries."
        />
    <TextView
        app:layout_constraintTop_toBottomOf="@id/info"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/t2"
        android:layout_height="wrap_content"
        android:textColor="#F0D453"
        android:textSize="42sp"
        android:layout_marginTop="24dp"
        android:textStyle="normal"
        android:layout_marginLeft="24dp"
        android:fontFamily="@font/montserratmedium"
        android:text="T"
        />
    <TextView
        app:layout_constraintStart_toEndOf="@id/t2"
        app:layout_constraintTop_toBottomOf="@id/info"
        android:layout_width="wrap_content"
        android:id="@+id/t1"
        android:layout_marginTop="24dp"
        android:textStyle="normal"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:textSize="42sp"
        android:fontFamily="@font/montserratmedium"
        android:text="eam"
        />
    <TextView
        app:layout_constraintTop_toBottomOf="@id/t1"
        android:layout_width="match_parent"
        android:layout_marginTop="6dp"
        android:id="@+id/info2"
        android:textColor="#FFFFFF"
        android:fontFamily="@font/montserratmedium"
        android:textSize="14sp"
        android:textStyle="normal"
        android:layout_marginHorizontal="24dp"
        android:layout_height="wrap_content"
        android:text="We are fans of digital, geeks by nature, addicted to social networks and completely crazy about design."
        />
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        app:layout_constraintTop_toBottomOf="@id/info2"
        android:id="@+id/cl2"
        android:layout_marginTop="30dp"
        android:layout_width="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#232323"
        android:layout_height="wrap_content">
        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cardViewS"
            android:layout_width="45.5pt"
            android:layout_marginRight="24dp"
            android:layout_height="45.5pt"
            android:src="@drawable/varunb"
            android:scaleType="centerCrop"
            app:civ_border_color="#F0D453"
            app:civ_border_width="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:textSize="7.2pt"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/montserratmedium"
            android:text="Varun Bhardwaj"
            android:textStyle="normal"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/cardViewS"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/cl3"
        android:layout_width="match_parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/cl2"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#232323"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cardView"
            android:layout_width="45.5pt"
            android:layout_marginLeft="24dp"
            android:layout_height="45.5pt"
            android:src="@drawable/varun"
            app:civ_border_color="#F0D453"
            android:scaleType="centerCrop"
            app:civ_border_width="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <TextView
            android:id="@+id/cordi_name"
            android:layout_width="wrap_content"
            android:textSize="7.2pt"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginRight="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="16dp"
            android:textStyle="normal"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/montserratmedium"
            android:text="Varun Tiwari"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cardView"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        app:layout_constraintTop_toBottomOf="@+id/cl3"
        android:id="@+id/cl4"
        android:layout_width="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#232323"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cardView4"
            android:layout_width="45.5pt"
            android:layout_marginRight="24dp"
            android:layout_height="45.5pt"
            android:src="@drawable/shreyas"
            app:civ_border_color="#F0D453"
            app:civ_border_width="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <TextView
            android:id="@+id/cordi_name_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:textSize="7.2pt"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/montserratmedium"
            android:text="Shreyas Gupta"
            android:textStyle="normal"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/cardView4"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/cl5"
        android:layout_width="match_parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/cl4"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#232323"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cardView5"
            android:layout_width="45.5pt"
            android:layout_marginLeft="24dp"
            android:layout_height="45.5pt"
            android:src="@drawable/chetan"
            app:civ_border_color="#F0D453"
            app:civ_border_width="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <TextView
            android:id="@+id/cordi_name-cg"
            android:layout_width="wrap_content"
            android:textSize="7.2pt"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginRight="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="16dp"
            android:textStyle="normal"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/montserratmedium"
            android:text="Chetan Garg"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cardView5"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        app:layout_constraintTop_toBottomOf="@+id/cl5"
        android:layout_marginTop="24dp"
        android:id="@+id/cl6"
        android:layout_width="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#232323"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cardView6"
            android:layout_width="45.5pt"
            android:layout_marginRight="24dp"
            android:layout_height="45.5pt"
            android:src="@drawable/anip"
            app:civ_border_color="#F0D453"
            android:scaleType="centerCrop"
            app:civ_border_width="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <TextView
            android:id="@+id/cordi_name_an"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:textSize="7.2pt"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/montserratmedium"
            android:text="Anip Kumar"
            android:textStyle="normal"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/cardView6"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/cl7"
        android:layout_width="match_parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/cl6"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#232323"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/cardView7"
            android:layout_width="45.5pt"
            android:layout_marginLeft="24dp"
            android:layout_height="45.5pt"
            android:src="@drawable/mitisha"
            android:scaleType="centerCrop"
            app:civ_border_color="#F0D453"
            app:civ_border_width="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />
        <TextView
            android:id="@+id/cordi_name_mt"
            android:layout_width="wrap_content"
            android:textSize="7.2pt"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginRight="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="16dp"
            android:textStyle="normal"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/montserratmedium"
            android:text="Mitisha Aggarwal"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cardView7"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>