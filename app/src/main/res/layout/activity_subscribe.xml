                    <TextView
                        android:layout_marginTop="8dp"
                        android:fontFamily="@font/montserratmedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="RANGTARANGINI"
                        android:gravity="center"
                        android:textSize="7.2pt"
                        android:layout_gravity="center"
                        android:textColor="#FFFFFF"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Dramatics Society"
                        android:gravity="center"
                        android:fontFamily="@font/montserratmedium"
                        android:textColor="#6B6B6B"
                        android:textSize="10sp"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1"
                android:layout_marginTop="24dp"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="155dp"
                    android:layout_marginRight="12dp"
                    android:layout_weight="0.5"
                    android:orientation="vertical"
                    android:id="@+id/virtuosi"
                    android:background="@drawable/society_card"
                    >
                    <ImageView
                        android:layout_width="90dp"
                        android:layout_marginTop="16dp"
                        android:layout_height="80dp"
                        android:id="@+id/virtuosi_logo"
                        android:layout_gravity="center"
                        android:layout_marginHorizontal="8dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/virtuosi"
                        />
                    <TextView
                        android:layout_marginTop="8dp"
                        android:fontFamily="@font/montserratmedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="VIRTUOSI"
                        android:gravity="center"
                        android:textSize="16sp"
                        android:layout_gravity="center"
                        android:textColor="#FFFFFF"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Musical Society"
                        android:gravity="center"
                        android:fontFamily="@font/montserratmedium"
                        android:textColor="#6B6B6B"
                        android:textSize="10sp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="155dp"
                    android:layout_weight="0.5"
                    android:orientation="vertical"
                    android:id="@+id/nirmiti"
                    android:layout_marginLeft="12dp"
                    android:background="@drawable/society_card"
                    >
                    <ImageView
                        android:layout_width="90dp"
                        android:layout_marginTop="16dp"
                        android:layout_height="80dp"
                        android:id="@+id/nirmiti_logo"
                        android:layout_gravity="center"
                        android:layout_marginHorizontal="8dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/nirmiti"
                        />
                    <TextView
                        android:layout_marginTop="8dp"
                        android:fontFamily="@font/montserratmedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="NIRMITI"
                        android:gravity="center"
                        android:textSize="16sp"
                        android:layout_gravity="center"
                        android:textColor="#FFFFFF"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Fine Arts Society"
                        android:gravity="center"
                        android:fontFamily="@font/montserratmedium"
                        android:textColor="#6B6B6B"
                        android:textSize="10sp"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="1"
            android:layout_marginTop="24dp"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="155dp"
                android:layout_marginRight="12dp"
                android:layout_weight="0.5"
                android:orientation="vertical"
                android:id="@+id/ams"
                android:background="@drawable/society_card"
                >
                <ImageView
                    android:layout_width="90dp"
                    android:layout_marginTop="16dp"
                    android:layout_height="80dp"
                    android:id="@+id/ams_logo"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="8dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/logoams"
                    />
                <TextView
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/montserratmedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="AMS"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Media Society"
                    android:gravity="center"
                    android:fontFamily="@font/montserratmedium"
                    android:textColor="#6B6B6B"
                    android:textSize="10sp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="155dp"
                android:layout_weight="0.5"
                android:orientation="vertical"
                android:id="@+id/geniticx"
                android:layout_marginLeft="12dp"
                android:background="@drawable/society_card"
                >
                <ImageView
                    android:layout_width="90dp"
                    android:layout_marginTop="16dp"
                    android:layout_height="80dp"
                    android:id="@+id/geniticx_logo"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="8dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/geneticx_white"
                    />
                <TextView
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/montserratmedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="GENITICX"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Dance Society"
                    android:gravity="center"
                    android:fontFamily="@font/montserratmedium"
                    android:textColor="#6B6B6B"
                    android:textSize="10sp"
                    />
            </LinearLayout>
        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="155dp"
                android:weightSum="1"
                android:orientation="horizontal"
                android:layout_marginTop="24dp"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="155dp"
                    android:layout_marginRight="12dp"
                    android:layout_weight="0.5"
                    android:orientation="vertical"
                    android:id="@+id/spirit"
                    android:background="@drawable/society_card"
                    >
                    <ImageView
                        android:layout_width="90dp"
                        android:layout_marginTop="16dp"
                        android:layout_height="80dp"
                        android:id="@+id/spirit_logo"
                        android:layout_gravity="center"
                        android:layout_marginHorizontal="8dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/spirit_white1"
                        />
                    <TextView
                        android:layout_marginTop="8dp"
                        android:fontFamily="@font/montserratmedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="SPIRIT"
                        android:gravity="center"
                        android:textSize="16sp"
                        android:layout_gravity="center"
                        android:textColor="#FFFFFF"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Sports Society"
                        android:gravity="center"
                        android:fontFamily="@font/montserratmedium"
                        android:textColor="#6B6B6B"
                        android:textSize="10sp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="155dp"
                    android:layout_weight="0.5"
                    android:orientation="vertical"
