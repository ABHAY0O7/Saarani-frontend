<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232323"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.constraintlayout.widget.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#232323">
        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/extended_fab"
            android:layout_width="match_parent"
            android:layout_marginHorizontal="44dp"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:paddingHorizontal="16dp"
            android:elevation="12dp"
            android:fontFamily="@font/montserratmedium"
            android:textStyle="bold"
            android:backgroundTint="#F5D22B"
            android:textAllCaps="false"
            android:textSize="20sp"
            android:text="Continue"
            android:textAlignment="center"
            android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:theme="@style/Theme.MaterialComponents.NoActionBar"
            tools:layout_editor_absoluteX="16dp" />

    <ScrollView
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="#232323"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:fontFamily="@font/montserratmedium"
            android:paddingLeft="24dp"
            android:paddingRight="24dp"
            android:paddingBottom="10dp"
            android:text="Follow Your Interest"
            android:textColor="#FFFFFF"
            android:textSize="50sp"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserratmedium"
            android:paddingLeft="24dp"
            android:text="Follow the societies you are interested in"
            android:textColor="#62615F"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:orientation="vertical"
            android:layout_marginHorizontal="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1"
                >
             <LinearLayout
                 android:layout_width="0dp"
                 android:id="@+id/geekhaven"
                 android:layout_height="155dp"
                 android:layout_marginRight="12dp"
                 android:layout_weight="0.5"
                 android:orientation="vertical"
                 android:background="@drawable/society_card">
                 <ImageView
                     android:layout_width="90dp"
                     android:layout_marginTop="16dp"
                     android:layout_height="80dp"
                     android:id="@+id/geekhaven_logo"
                     android:layout_gravity="center"
                     android:layout_marginHorizontal="8dp"
                     android:scaleType="centerInside"
                     android:background="@drawable/ic_gh_white"
                 />
                 <TextView
                     android:layout_marginTop="8dp"
                     android:fontFamily="@font/montserratmedium"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:text="GEEKHAVEN"
                     android:gravity="center"
                     android:textSize="16sp"
                     android:layout_gravity="center"
                     android:textColor="#FFFFFF"/>
                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:text="Technical Society"
                     android:gravity="center"
                     android:fontFamily="@font/montserratmedium"
                     android:textColor="#6B6B6B"
                     android:textSize="10sp"
                     />
             </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="155dp"
                    android:layout_weight="0.5"
                    android:id="@+id/tesla"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:background="@drawable/society_card"
                    >
                    <ImageView
                        android:layout_width="90dp"
                        android:layout_marginTop="16dp"
                        android:layout_height="80dp"
                        android:id="@+id/tesla_logo"
                        android:layout_gravity="center"
                        android:layout_marginHorizontal="8dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/tesla"
                        />
                    <TextView
                        android:layout_marginTop="8dp"
                        android:fontFamily="@font/montserratmedium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="TESLA"
                        android:gravity="center"
                        android:textSize="16sp"
                        android:layout_gravity="center"
                        android:textColor="#FFFFFF"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Technical Society"
                        android:gravity="center"
                        android:fontFamily="@font/montserratmedium"
                        android:textColor="#6B6B6B"
                        android:textSize="10sp"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:weightSum="1"
                >
                <LinearLayout
